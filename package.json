{
  "name": "@10up/sanity-starter",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "next:dev": "next dev --turbopack",
    "next:build": "next build",
    "next:start": "next start",
    "next:debug": "NEXT_PRIVATE_DEBUG_CACHE=1 next:build && next:start",
    "lint": "next lint",
    "dev": "npm-run-all --parallel --race next:dev watch-typegen",
    "sanity:typegen": "sanity schema extract --path=.sanity/schema.json && sanity typegen generate",
    "typecheck": "tsc --noEmit",
    "watch-typegen": "tsx watch-typegen.ts"
  },
  "dependencies": {
    "@sanity/assist": "^3.0.8",
    "@sanity/icons": "^3.4.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/react-loader": "^1.10.28",
    "@sanity/vision": "^3.64.0",
    "clsx": "^2.1.1",
    "next": "^15.0.3",
    "next-sanity": "^9.8.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^3.64.0",
    "server-only": "^0.0.1",
    "sonner": "^1.7.1",
    "styled-components": "^6.1.13",
    "tailwind-merge": "^2.5.5",
    "valibot": "^1.0.0-beta.14"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/chokidar": "^2.1.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "chokidar": "^4.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.1",
    "eslint-plugin-import": "^2.31.0",
    "groq": "^3.72.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}

{
  "name": "@10up/sanity-ignite",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "next:dev": "next dev --turbopack",
    "next:build": "npm run sanity:typegen && next build",
    "next:start": "next start",
    "next:debug": "NEXT_PRIVATE_DEBUG_CACHE=1 next:build && next:start",
    "lint": "next lint",
    "dev": "npm-run-all --parallel --race next:dev watch-typegen",
    "built": "npm run next:build",
    "start": "npm run next:start",
    "sanity:typegen": "mkdir -p ./.sanity && sanity schema extract --path=.sanity/schema.json && sanity typegen generate",
    "typecheck": "tsc --noEmit",
    "watch-typegen": "tsx watch-typegen.ts",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-slot": "^1.1.2",
    "@sanity/assist": "^3.0.8",
    "@sanity/icons": "^3.4.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/react-loader": "^1.10.28",
    "@sanity/vision": "^3.64.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.476.0",
    "next": "^15.0.3",
    "next-sanity": "^9.8.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^3.64.0",
    "server-only": "^0.0.1",
    "sonner": "^1.7.1",
    "styled-components": "^6.1.13",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^1.0.0-beta.14"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/experimental-addon-test": "^8.6.4",
    "@storybook/experimental-nextjs-vite": "^8.6.4",
    "@storybook/nextjs": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/chokidar": "^2.1.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "chokidar": "^4.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-storybook": "^0.11.4",
    "groq": "^3.72.1",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.51.0",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "storybook": "^8.6.4",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
